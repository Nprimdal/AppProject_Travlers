<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.AppProject_Travlers" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/granny_with_apple</item>
        <item name="colorPrimaryVariant">@color/light_moss_green</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/dirty_green_white</item>
        <item name="colorSecondaryVariant">@color/light_beige</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="android:starStyle">@color/Orange</item>
        <item name="backgroundColor">@color/light_beige</item>
    </style>
</resources>